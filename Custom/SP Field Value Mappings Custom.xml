<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE Custom PUBLIC "sailpoint.dtd" "sailpoint.dtd">
<Custom name="SP Field Value Mappings Custom">
  <Attributes>
    <Map>
      <entry key="Active Directory">
        <value>
          <List>
            <Field name="distinguishedName" type="rule" value="getDistinguishedName"/>
            <Field name="sAMAccountName" type="attribute" value="name"/>
            <Field name="userPrincipalName" type="rule" value="getUserPrincipalName"/>
            <Field name="password" type="rule" value="getADPassword"/>
            <Field name="givenName" type="attribute" value="firstname"/>
            <Field name="sn" type="attribute" value="lastname"/>
            <Field name="displayName" type="rule" value="getADDisplayName"/>
            <Field name="mail" type="rule" value="getEmail"/>
            <Field name="title" type="attribute" value="jobTitle"/>
            <Field name="department" type="attribute" value=""/>
            <Field name="company" type="attribute" value="company"/>
            <Field name="manager" type="rule" value="getADManagerDN"/>
            <Field name="l" type="attribute" value="location"/> 
			<Field name="telephoneNumber" type="attribute" value=""/>
            <Field name="mailNickname" type="rule" value="getMailNickname"/>
            <Field name="homeMDB" type="rule" value="getExchHomeMDB"/>

          </List>
        </value>
      </entry>
    </Map>
  </Attributes>
</Custom>